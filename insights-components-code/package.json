{
  "name": "@Insights-App/insights-components",
  "version": "2.0.15",
  "description": "shared assets and components for insights app",
  "author": "akash-singh-au6",
  "license": "MIT",
  "repository": "https://github.com/Insights-App/insights-components",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "storybook": "start-storybook -p 6006",
    "start": "npm run build:style && microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "build-storybook": "build-storybook",
    "release": "standard-version"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "craco-plugin-scoped-css": "^1.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-scripts": "^3.4.1",
    "tailwindcss": "^1.8.10"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "filepond": "^4.20.1",
    "filepond-plugin-file-validate-type": "^1.2.5",
    "filepond-plugin-image-preview": "^4.6.4",
    "react-filepond": "^7.1.0",
    "react-player": "^2.6.2",
    "react-router-dom": "^5.2.0",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^4.4.1",
    "standard-version": "^9.0.0"
  }
}
